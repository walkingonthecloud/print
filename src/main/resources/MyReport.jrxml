<jasperReport name="MyReport">
    <property name="com.jaspersoft.studio.data.defaultdataadapter" value="wsi" />
    <parameter name="labelImage" class="java.io.InputStream" />
    <queryString>
        <![CDATA[select * from collates]]>
    </queryString>
    <title>
        <band height="700" >
            <staticText>
                <reportElement x="40" y="4" width="210" height="30" />
                <textElement textAlignment="Center" verticalAlignment="Middle" />
                <text>
                    <![CDATA[This is image example in jasper report]]>
                </text>
            </staticText>
            <image>
                <reportElement x="40" y="14" width="350" height="500" />
                <imageExpression>
                    <![CDATA[$P{labelImage}]]>
                </imageExpression>
            </image>
        </band>
    </title>
</jasperReport>